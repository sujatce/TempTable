GetFileName([FullPath])




DateTimeParse(
    REGEX_Replace(GetFileName([FullPath]), ".*_(\\d{8})\\.txt", "$1"),
    "%Y%m%d"
)



REGEX_Replace([FileName], ".*[\\\\/]", "")

DateTimeParse(
  REGEX_Replace([ONLY_FILE_NAME], ".*_([0-9]{8})\\.[Tt][Xx][Tt]$", "$1"),
  "%Y%m%d"
)



DateTimeParse(
  REGEX_Replace([FileName], ".*_([0-9]{8})\\.[Tt][Xx][Tt]$", "$1"),
  "%Y%m%d"
)



REGEX_Replace([FILE_NAME], ".*_([0-9]{8})\\.[Tt][Xx][Tt]$", "$1")
