SELECT
    mrm.GMPI_ID,
    dm.MEMBER_MBI,
    mrm.MBR_DATE_OF_BIRTH AS MEMBER_BIRTH_DATE,
    mrm.MBR_ELIG_INSURANCE_COMPANY,
    mrm.CONTRACT_ID,
    mrm.CONTRACT_PBP
FROM BI_Analysis.dbo.MEMBER_REPORT_MONTH AS mrm
INNER JOIN ORCHESTRATION.dbo.DIM_MEMBER AS dm
    ON mrm.GMPI_ID = dm.GMPI_ID
WHERE mrm.DATA_ENTITY = 'WellMed'
  AND mrm.REPORT_MONTH = '2025-09-01'
  AND mrm.ACTIVE_FLAG = 1;



select distinct ASQN_DEV_ORCHESTRATION_DB.DBO.SMS_OPT_OUT.FILE_NAME as FILE_NAME 
from ASQN_DEV_ORCHESTRATION_DB.DBO.SMS_OPT_OUT

union all

select Cast Null as varchar(50) as FILE_NAME
where not exists 
	(
	select 1 
	from ASQN_DEV_ORCHESTRATION_DB.DBO.SMS_OPT_OUT
	)
