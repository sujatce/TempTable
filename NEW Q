SELECT
    mrm.GMPI_ID,
    dm.MEMBER_MBI AS MBI,
    mrm.MBR_DATE_OF_BIRTH AS MEMBER_BIRTH_DATE,
    mrm.MBR_ELIG_INSURANCE_COMPANY,
    mrm.CONTRACT_ID,
    mrm.CONTRACT_PBP
FROM FWRB_PRD_WMIDR_DB.BI_ANALYSIS.MEMBER_REPORT_MONTH AS mrm
INNER JOIN FWRB_PRD_WMIDR_DB.BI_ANALYSIS.DIM_MEMBER AS dm
    ON mrm.GMPI_ID = dm.GMPI_ID
WHERE mrm.DATA_ENTITY = 'Wellmed'
  AND mrm.REPORT_MONTH = '2025-10-01'
  AND mrm.ACTIVE_FLAG = 1;
