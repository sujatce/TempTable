select *
from FWRB_PRD_WMIDR_DB.BI_ANALYSIS.DEFINITION_RESULT_ACTIVE
where report_month = '2025-12-01'
AND RESULT_DATE >='2025-12-01'
AND UPPER(claim_specialty_value) like '%EMERGENCY%';


create or replace ICEBERG TABLE FWRB_PRD_WMIDR_DB.BI_ANALYSIS.DEFINITION_RESULT_ACTIVE (
	DEFINITION_MONTH_KEY DECIMAL(15, 0) NOT NULL,
	DEFINITION_ID DECIMAL(15, 0) NOT NULL,
	DEFINITION_CODE STRING NOT NULL,
	REPORT_MONTH DATE NOT NULL,
	PATIENT_ID DECIMAL(15, 0),
	MEMBER_ID DECIMAL(15, 0),
	GMPI_ID DECIMAL(38, 0) NOT NULL,
	RESULT_DATE DATE,
	RESULT_CODE STRING,
	RESULT_VALUE STRING,
	SRC_TBL_ID DECIMAL(15, 0) NOT NULL,
	SRC_TBL_NM STRING NOT NULL,
	DW_LAST_EXECUTION_ID DECIMAL(15, 0),
	PRIMARY_ID DECIMAL(15, 0),
	SECONDARY_ID DECIMAL(15, 0),
	PRIMARY_DATE DATE,
	IS_TELEHEALTH DECIMAL(38, 0),
	IS_EXCLUSION DECIMAL(38, 0),
	IS_BILATERAL DECIMAL(38, 0),
	IS_LEFT DECIMAL(38, 0),
	IS_RIGHT DECIMAL(38, 0),
	DATA_SOURCE STRING,
	CODE_TYPE STRING,
	CLAIM_SPECIALTY_VALUE STRING,
	DW_UPDATE_DATE TIMESTAMP_NTZ(6)
)COMMENT='Large_Loads'

 EXTERNAL_VOLUME = 'FWRB_PRD_SIT_EXT_VOL_BRONZE_ICEBERG_EXVOL'
 CATALOG = 'SNOWFLAKE'
 BASE_LOCATION = 'SQLServer/SQL_BI_ANALYSIS_BI01_PRD/DBO/DEFINITION_RESULT_ACTIVE/';
