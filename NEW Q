select *
from FWRB_PRD_WMIDR_DB.BI_ANALYSIS.DEFINITION_RESULT_ACTIVE
where report_month = '2025-12-01'
AND RESULT_DATE >='2025-12-01'
AND UPPER(claim_specialty_value) like '%EMERGENCY%';
