SELECT  
    s.name  AS schema_name,
    p.name  AS procedure_name,
    p.create_date,
    p.modify_date
FROM sys.procedures p
JOIN sys.schemas s
    ON p.schema_id = s.schema_id
WHERE p.name LIKE '%EPMP%'
ORDER BY p.name;



SELECT  
    s.name  AS schema_name,
    p.name  AS procedure_name,
    p.create_date,
    p.modify_date
FROM sys.procedures p
JOIN sys.schemas s
    ON p.schema_id = s.schema_id
WHERE s.name LIKE '%EPMP%'
ORDER BY p.name;
