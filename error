
select x.GMPI_ID,
	'Y' as NEW_MEMBER_FLAG 
from 
	(
	select FWRB_PRD_WMIDR_DB.BI_ANALYSIS.MEMBER_REPORT_MONTH.GMPI_ID 
	from FWRB_PRD_WMIDR_DB.BI_ANALYSIS.MEMBER_REPORT_MONTH 
	where FWRB_PRD_WMIDR_DB.BI_ANALYSIS.MEMBER_REPORT_MONTH.REPORT_MONTH ='2026-01-01'Â 

except

	select FWRB_PRD_WMIDR_DB.BI_ANALYSIS.MEMBER_REPORT_MONTH.GMPI_ID 
	from FWRB_PRD_WMIDR_DB.BI_ANALYSIS.MEMBER_REPORT_MONTH
	where FWRB_PRD_WMIDR_DB.BI_ANALYSIS.MEMBER_REPORT_MONTH.REPORT_MONTH = dateadd(month,-6,'2026-01-01'
)) x
