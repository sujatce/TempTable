create or replace TABLE ASQN_DEV_ORCHESTRATION_DB.DBO.NEW_TO_THERAPY (
	NEW_TO_THERAPY_ID NUMBER(38,0) NOT NULL DEFAULT ASQN_DEV_ORCHESTRATION_DB.DBO.NEW_TO_THERAPY_SEQ.NEXTVAL COMMENT 'Auto-generated Unique ID.',
	RPT_MO_KEY VARCHAR(10) COMMENT 'This field captures the Year and Month of when the record was created.',
	REPORTDATE DATE COMMENT 'Last Empanelment Date.',
	HCONTRACT VARCHAR(50) COMMENT 'Contract Number, typically beginning with H and followed by 4 digit number.',
	CDO VARCHAR(50) COMMENT 'Care Delivery Organization - is a legal entity that delivers healthcare services and products.',
	CDO_SUBGROUP VARCHAR(75) COMMENT 'Sub division of CDOs in a region.',
	YEAR_DOS NUMBER(38,0) COMMENT 'Year of Date of Service - Captures the Year for which data is relevant.',
	MONTH_DOS NUMBER(38,0) COMMENT 'Month of Date of Service- Captures the Month of the year for which data is relevant.',
	BYY NUMBER(38,0) COMMENT 'Best Year Yet. This is a Flag that is associated to OPTUM population (mostly global risk), sample value - 1.',
	MGMT_TYPE VARCHAR(25) COMMENT 'Identifies if a member belongs to OptumCare or Optum at Home. Sample values - OC, OC_OAH.',
	PBP VARCHAR(5) COMMENT 'Plan Benefit Package-A set of benefits for a defined MA or PDP service area. ',
	PCP_NPI VARCHAR(25) COMMENT 'Primary Care Provider National Provider Identifier.',
	ENTY_ID NUMBER(38,0) COMMENT 'Enity ID-Numeric identifier that identifies a CDO. Sample values - 30063, 30010.',
	PCP_TIN VARCHAR(25) COMMENT 'Primary Care Provider National Provider Identifier Tax ID Number.',
	EMP_STATUS VARCHAR(50) COMMENT 'This field identifies if PCP is Employed/Contracted/Affiliated.Example values - E, C, A.',
	RISK_TYPE VARCHAR(50) COMMENT 'This field captures the financial cost structure associated with member. Example values - GLOBAL, FFS.',
	MEASURE_YEAR NUMBER(38,0) COMMENT 'It is the period of time when patients are receiving their screenings, filling prescriptions and responding to surveys.                                        The measurement period is literally the calendar year Jan. 1 - Dec. 31.',
	CONTRACT_PBP VARCHAR(100) COMMENT 'Concatenated value of HContract and PBP.',
	SUPER_CDO VARCHAR(75) COMMENT 'Historical reference to CDO for CA region, place holder for that data.',
	SUPER_CDO_SUBGROUP VARCHAR(75) COMMENT 'Historical reference to CDO for CA region, place holder for that data.',
	MAD_FILLS_2Y NUMBER(38,0) COMMENT 'Number of MAD medication fills in the past 2 years.',
	MAD_LATEST_FILL_DATE DATE COMMENT 'Last fill date for MAD medication.',
	MAD_DAYS_SINCE_LAST_FILL NUMBER(38,0) COMMENT 'Days since MAD medication was last filled.',
	MAD_NEW_TO_THERAPY NUMBER(38,0) COMMENT 'New to Therapy members for MAD are identified as having a first fill of a medication adherence medication in the last 30-days and showing no history of being a part of the measure within the past 2 years.',
	MAD_FILLS_180D NUMBER(38,0) COMMENT 'Number of MAD medication fills in the past 180 days.',
	MAD_NTT_RESTART NUMBER(38,0) COMMENT 'New to Therapy restart members for MAD are identified as having a first fill of a medication adherence medicationin the last 30-days and has a history of being a part of the measure within the past 2 years but no fills within the past 6 months.',
	MAD_LATEST_FILL_DAYS_SUPPLY NUMBER(38,0) COMMENT 'Days supply covered in the last fill for MAD medication.',
	MAD_LATEST_FILL_QUANTITY NUMBER(38,0) COMMENT 'Quantity of the drugs bought in last fill for MAD medication.',
	MAC_FILLS_2Y NUMBER(38,0) COMMENT 'Number of MAC medication fills in the past 2 years.',
	MAC_LATEST_FILL_DATE DATE COMMENT 'Last fill date for MAC medication.',
	MAC_DAYS_SINCE_LAST_FILL NUMBER(38,0) COMMENT 'Days since MAC medication was last filled.',
	MAC_NEW_TO_THERAPY NUMBER(38,0) COMMENT 'New to Therapy members for MAC are identified as having a first fill of a medication adherence medication in the last 30-days and showing no history of being a part of the measure within the past 2 years.',
	MAC_FILLS_180D NUMBER(38,0) COMMENT 'Number of MAC medication fills in the past 180 days.',
	MAC_NTT_RESTART NUMBER(38,0) COMMENT 'New to Therapy restart members for MAC are identified as having a first fill of a medication adherence medication in the last 30-days and has a history of being a part of the measure within the past 2 years but no fills within the past 6 months.',
	MAC_LATEST_FILL_DAYS_SUPPLY NUMBER(38,0) COMMENT 'Days supply covered in the last fill for MAC medication.',
	MAC_LATEST_FILL_QUANTITY NUMBER(38,0) COMMENT 'Quantity of the drugs bought in last fill for MAC medication.',
	MAH_FILLS_2Y NUMBER(38,0) COMMENT 'Number of MAH medication fills in the past 2 years.',
	MAH_LATEST_FILL_DATE DATE COMMENT 'Last fill date for MAH medication.',
	MAH_DAYS_SINCE_LAST_FILL NUMBER(38,0) COMMENT 'Days since MAH medication was last filled.',
	MAH_NEW_TO_THERAPY NUMBER(38,0) COMMENT 'New to Therapy members for MAH are identified as having a first fill of a medication adherence medication in the last 30-days and showing no history of being a part of the measure within the past 2 years.',
	MAH_FILLS_180D NUMBER(38,0) COMMENT 'Number of MAH medication fills in the past 180 days.',
	MAH_NTT_RESTART NUMBER(38,0) COMMENT 'New to Therapy restart members for MAH are identified as having a first fill of a medication adherence medication in the last 30-days and has a history of being a part of the measure within the past 2 years but no fills within the past 6 months.',
	MAH_LATEST_FILL_DAYS_SUPPLY NUMBER(38,0) COMMENT 'Days supply covered in the last fill for MAH medication.',
	MAH_LATEST_FILL_QUANTITY NUMBER(38,0) COMMENT 'Quantity of the drugs bought in last fill for MAH medication.',
	GMPI_ID NUMBER(38,0) COMMENT 'Member identifier internal to Wellmed.',
	MBI VARCHAR(25) COMMENT 'Medicare Beneficiary Identifier.',
	ENCRYPT VARCHAR(25) COMMENT 'Flag to Identify if member is Offshore/Onshore.',
	REPORT_DATE DATE COMMENT 'Date when the process runs and creates the output.',
	OC_FLAG BOOLEAN COMMENT 'OptumCare flag, used for identifying OptumCare members',
	EMR_FLAG BOOLEAN COMMENT 'Flag to determine a Home and Community member.',
	HC_OC_COMANAGED_FLAG BOOLEAN COMMENT 'This flag indicates if a member is co-managed by Home & Community (HC) and OptumCare (OC).',
	ENTERPRISE_INDV_ID VARCHAR(25) COMMENT 'Enterprise Individual ID.',
	CLAIMS_REFRESH_DATE DATE COMMENT 'Refresh Date of the claim.',
	MAC_NTT_GENERIC VARCHAR(75) COMMENT 'The generic component associated with the therapy, if member is new to therapy for MAC medication.',
	MAD_NTT_GENERIC VARCHAR(75) COMMENT 'The generic component associated with the therapy, if member is new to therapy for MAD medication.',
	MAH_NTT_GENERIC VARCHAR(75) COMMENT 'The generic component associated with the therapy, if member is new to therapy for MAH medication.',
	MAC_NTT_MED_NAME VARCHAR(250) COMMENT 'The drug name associated with the therapy, if member is new to therapy for MAC medication.',
	MAD_NTT_MED_NAME VARCHAR(250) COMMENT 'The drug name associated with the therapy, if member is new to therapy for MAD medication.',
	MAH_NTT_MED_NAME VARCHAR(250) COMMENT 'The drug name associated with the therapy, if member is new to therapy for MAH medication.',
	PROCESS_LOG_ID NUMBER(38,0) COMMENT 'DB specific field.',
	LOAD_DATETIME VARCHAR(50) COMMENT 'Date and Time when the record was loaded into this table.',
	RECORD_ID NUMBER(38,0) COMMENT 'This is a DB field. It captures Id assigned to each inserted record in the table.',
	PATIENT_MISMATCH BOOLEAN COMMENT 'Flag to indicate if the member has a matching record in BI Analysis.',
	USER_STORY VARCHAR(25) COMMENT 'USER STORY associated with  creation - or with storing the data.',
	INSERT_DATE TIMESTAMP_NTZ(7) NOT NULL DEFAULT CAST(CONVERT_TIMEZONE('America/Chicago', CAST(CURRENT_TIMESTAMP() AS TIMESTAMP_TZ(9))) AS TIMESTAMP_NTZ(7)) COMMENT 'DATE data in the row was first inserted.',
	INSERT_USER VARCHAR(75) NOT NULL DEFAULT CURRENT_USER() COMMENT 'USER ID that first inserted data in the row.',
	UPDATE_DATE TIMESTAMP_NTZ(7) NOT NULL DEFAULT CAST(CONVERT_TIMEZONE('America/Chicago', CAST(CURRENT_TIMESTAMP() AS TIMESTAMP_TZ(9))) AS TIMESTAMP_NTZ(7)) COMMENT 'DATE data in the row was last updated.',
	UPDATE_USER VARCHAR(75) NOT NULL DEFAULT CURRENT_USER() COMMENT 'USER ID that last updated data in the row.',
	constraint PK_NEW_TO_THERAPY primary key (NEW_TO_THERAPY_ID)
)COMMENT='This dataset contains information around Optum Home and Community membership, MAD, MAC and MAH refill dates and Optum health membership indicator.\nIt enhances existing New To Therapy calculations for the Optum Home and Community delegated members in the Medicare and Retirement (M&R) population.\n\nData is used to identify members who are receiving a new prescription for a MAC/MAD/MAH measure medication where they either have no prior claim history of that medication, or have no evidence of a paid claim within the past 6 months.'
;
